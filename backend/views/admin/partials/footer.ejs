<script>

    function getState(id) {
        var id = id;
        alert(id);
        if (id != "") {
            $.ajax({
                method: "GET",
                url: "/auser/state/" + id,
                data: JSON.stringify(),
            }).done(function (return_data) {
                alert(return_data);
                $("#state").html("<option value=''>--- Select state ---</option>" + return_data);
            }).error(function () {
                alert("error");
            });
        } else {
            alert("Error");
        }
    }

    function getcity() {
        var country_id = $('#country').val();
        var state_id = $('#state').val();

        alert("country_id = " + country_id + " state_id = " + state_id);

        if (country_id != "" && state_id != "") {
            $.ajax({
                method: "GET",
                url: "/auser/city/" + state_id + "/" + country_id,
                data: JSON.stringify()
                , success: function (data) {
                    alert(JSON.stringify(data));
                }
            }).done(function (return_data) {
                alert(return_data);
                $("#city").html("<option value=''>--- Select city ---</option>" + return_data);
            }).error(function () {
                alert("error");
            });
        } else {
            alert("Error");
        }
    }

    function getcountrydata() {
        $.ajax({
            method: "GET",
            url: "/edit_user/countrydata",
            data: JSON.stringify()
            /*success:function (data){
             alert(JSON.stringify(data));
         }*/
        }).done(function (return_data) {
            alert(return_data);
            $("#country").html("<option value=''>--- Select Country ---</option>" + return_data);
        }).error(function () {
            alert("error");
        });
    }

    function gethobbiesdata() {
        $(function () {
            $("#btnGet").click(function () {
                //Create an Array.
                var selected = new Array();

                //Reference the CheckBoxes and insert the checked CheckBox value in Array.
                $("#hobbies input[type=checkbox]:checked").each(function () {
                    selected.push(this.value);
                });

                //Display the selected CheckBox values.
                if (selected.length > 0) {
                    alert("Selected values: " + selected.join(","));
                }

            });
        });
    }

</script>
